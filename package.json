{
  "name": "d3-axis-for-react",
  "version": "1.1.0",
  "description": "A verbatim d3 axis component for React",
  "main": "dist/index.js",
  "author": "Tom MacWright",
  "license": "MIT",
  "source": "src/index.tsx",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "umd:main": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "tsc && jest",
    "build": "microbundle  --jsx React.createElement --no-compress && npm run doc",
    "doc": "typedoc --plugin typedoc-plugin-markdown --readme none src/index.tsx",
    "prepare": "npm run build",
    "release": "standard-version"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.2",
    "@babel/preset-react": "^7.22.3",
    "@babel/preset-typescript": "^7.21.5",
    "@types/d3": "^7.4.0",
    "@types/d3-axis": "^3.0.2",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.2.7",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.5.0",
    "d3": "^7.8.4",
    "eslint": "^8.41.0",
    "jest": "^29.5.0",
    "microbundle": "^0.15.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "standard-version": "^9.5.0",
    "typedoc": "^0.24.7",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "^5.0.4"
  }
}
